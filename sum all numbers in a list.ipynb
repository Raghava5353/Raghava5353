   "source": [
    "def sum_numbers(numbers):\n",
    "    total = 0\n",
    "    for num in numbers:\n",
    "        total += num\n",
    "    return total\n",
    "\n",
    "# Example usage\n",
    "sample_list = (8, 2, 3, 0, 7)\n",
    "result = sum_numbers(sample_list)\n",
    "print(result)\n"
   ]
